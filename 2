#include <iostream>
#include <string>
#include <vector>
#include <deque>

int main() {
    int n;
    std::cin >> n;

    std::deque<std::string> stack;

    for (int i = 0; i < n; ++i) {
        std::string name;
        std::string position;
        std::cin >> name >> position;

        if (position == "top") {
            stack.push_front(name);
        } else {
            stack.push_back(name);
        }
    }

    int m;
    std::cin >> m;

    for (int i = 0; i < m; ++i) {
        int index;
        std::cin >> index;
        std::cout << stack[index - 1] << "\n";
    }
}
